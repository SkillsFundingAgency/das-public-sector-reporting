@model SFA.DAS.PSRService.Web.ViewModels.QuestionViewModel

@{
    ViewData["Title"] = "Public sector reporting";
}

<div class="grid-row">
    <div class="column-two-thirds">
        <h1 class="heading-xlarge">@Model.CurrentSection.Title</h1>
        
        
        @foreach (var question in Model.CurrentSection.Questions)
        {
            @Html.Partial(@Model.CurrentSection.Id)
        }
        <h2 class="heading-xlarge">Question Title</h2>
        <div style="white-space: pre">@Model</div>
        <button type="submit">Continue</button>
    </div>
</div>
@foreach (var section in Model.Report.Sections)
{

    @Html.Partial("Section", section)
}
