@using SFA.DAS.PSRService.Domain.Enums
@model SFA.DAS.PSRService.Web.ViewModels.CompletionStatusViewModel


@switch (Model.CompletionStatus)
{
    case CompletionStatus.Completed:
        <strong class="task-completed" id="@($"{Model.Id}")">Completed</strong>
        break;
    case CompletionStatus.InProgress:
        @*<strong class="task-completed" id="@($"{Model.Id}")">In Progress</strong>*@
        break;
    case CompletionStatus.Incomplete:
        @*<strong class="task-incomplete"  id="@($"{Model.Id}")">Incomplete</strong>*@
        break;
}

