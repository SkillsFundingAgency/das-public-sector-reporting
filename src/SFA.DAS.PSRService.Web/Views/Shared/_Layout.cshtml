@using SFA.DAS.PSRService.Web.Extensions
<!DOCTYPE html>
<html lang="en">
<head>
    @Html.Partial("_GoogleTagManager")
    <title>@ViewData["Title"]</title>

    <environment include="Development">
        <!-- TEMPLATE STYLES - START -->
        <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template.css?0.23.0" /><!--<![endif]-->
        <!--[if IE 6]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie6.css?0.23.0" /><![endif]-->
        <!--[if IE 7]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie7.css?0.23.0" /><![endif]-->
        <!--[if IE 8]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie8.css?0.23.0" /><![endif]-->
        <link rel="stylesheet" media="print" href="/assets/stylesheets/govuk-template-print.css?0.23.0" />
        <!--[if IE 8]><link rel="stylesheet" media="all" href="/assets/stylesheets/fonts-ie8.css?0.23.0" /><![endif]-->
        <!--[if gte IE 9]><!--><link rel="stylesheet" media="all" href="/assets/stylesheets/fonts.css?0.23.0" /><!--<![endif]-->
        <!-- TEMPLATE STYLES - END -->
        <!-- ELEMENTS AND TOOLKIT STYLES - START -->
        <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-base.css?3.1.2" /><!--<![endif]-->
        <!--[if IE 6]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie6.css?3.1.2" /><![endif]-->
        <!--[if IE 7]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie7.css?3.1.2" /><![endif]-->
        <!--[if IE 8]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie8.css?3.1.2" /><![endif]-->
        <!-- ELEMENTS AND TOOLKIT STYLES - END -->

        <link rel="stylesheet" href="~/css/site.css" />
    </environment>

    <environment exclude="Development">
        <!-- TEMPLATE STYLES - START -->
        <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template.min.css?0.23.0" /><!--<![endif]-->
        <!--[if IE 6]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie6.min.css?0.23.0" /><![endif]-->
        <!--[if IE 7]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie7.min.css?0.23.0" /><![endif]-->
        <!--[if IE 8]><link rel="stylesheet" media="screen" href="/assets/stylesheets/govuk-template-ie8.min.css?0.23.0" /><![endif]-->
        <link rel="stylesheet" media="print" href="/assets/stylesheets/govuk-template-print.min.css?0.23.0" />
        <!--[if IE 8]><link rel="stylesheet" media="all" href="/assets/stylesheets/fonts-ie8.min.css?0.23.0" /><![endif]-->
        <!--[if gte IE 9]><!--><link rel="stylesheet" media="all" href="/assets/stylesheets/fonts.min.css?0.23.0" /><!--<![endif]-->
        <!-- TEMPLATE STYLES - END -->
        <!-- ELEMENTS AND TOOLKIT STYLES - START -->
        <!--[if gt IE 8]><!--><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-base.min.css?3.1.2" /><!--<![endif]-->
        <!--[if IE 6]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie6.min.css?3.1.2" /><![endif]-->
        <!--[if IE 7]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie7.min.css?3.1.2" /><![endif]-->
        <!--[if IE 8]><link rel="stylesheet" media="screen" href="/assets/stylesheets/esfa-govuk-ie8.min.css?3.1.2" /><![endif]-->
        <!-- ELEMENTS AND TOOLKIT STYLES - END -->

        <link rel="stylesheet" href="~/css/site.min.css" />
    </environment>

    <!--[if lt IE 9]><script src="/assets/javascripts/ie.js?0.23.0"></script><![endif]-->

    <link rel="shortcut icon" href="/assets/images/favicon.ico" type="image/x-icon" />

    <link rel="mask-icon" href="/assets/images/gov.uk_logotype_crown.svg" color="#0b0c0c">
    <link rel="apple-touch-icon" sizes="180x180" href="/assets/images/apple-touch-icon-180x180.png">
    <link rel="apple-touch-icon" sizes="167x167" href="/assets/images/apple-touch-icon-167x167.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/assets/images/apple-touch-icon-152x152.png">
    <link rel="apple-touch-icon" href="/assets/images/apple-touch-icon.png">

    <meta name="theme-color" content="#0b0c0c" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:image" content="/assets/images/opengraph-image.png">
    @Html.Partial("_GoogleAnalyticsInfo")
    @Html.Partial("_ZenDeskWidget")
</head>
<body>
@Html.Partial("_GoogleTagManagerNoScript")
<script>document.body.className =
    ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

<div id="skiplink-container">
    <div>
        <a href="#content" class="skiplink">Skip to main content</a>
    </div>
</div>

@Html.Partial("_CookieBanner")

@Html.Partial("_Header", Html.GetHeaderViewModel())

<main id="content" role="main">
    @RenderBody()
</main>

@Html.Partial("_Footer", Html.GetFooterViewModel())

<div id="global-app-error" class="app-error hidden"></div>

<environment include="Development">
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/js/details.polyfill.js" asp-append-version="true"></script>
    <script src="~/js/shim-links-with-button-role.js" asp-append-version="true"></script>
    <script src="~/js/show-hide-content.js" asp-append-version="true"></script>
    <script src="~/js/cookies.js" asp-append-version="true"></script>
    <script src="~/js/site-nav.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
</environment>

<environment exclude="Development">
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/js/site.min.js" asp-append-version="true"></script>
</environment>

@RenderSection("Scripts", required: false)
<script>if (
    typeof window.GOVUK === 'undefined') document.body.className = document.body.className.replace('js-enabled', '');</script>
</body>
</html>