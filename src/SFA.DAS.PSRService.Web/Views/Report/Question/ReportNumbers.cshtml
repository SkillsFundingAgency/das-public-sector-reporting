@model SFA.DAS.PSRService.Domain.Entities.Section
@{
    ViewBag.GaData.Vpv = "/public-sector-reporting/report/question/page-report-numbers";
}

<partial name="Question/YourEmployees" model="@Model.SubSections.Single(w => w.Id == "YourEmployees")"/>
<partial name="Question/YourApprentices" model="@Model.SubSections.Single(w => w.Id == "YourApprentices")"/>
@if (Model.SubSections.SingleOrDefault(w => w.Id == "FullTimeEquivalent") != null)
{
    <partial name="Question/FullTimeEquivalent" model="@Model.SubSections.Single(w => w.Id == "FullTimeEquivalent")"/>
}