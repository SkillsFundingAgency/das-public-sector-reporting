@model SFA.DAS.PSRService.Domain.Entities.Section
@{
    ViewBag.GaData.Vpv = "/public-sector-reporting/report/question/page-report-numbers";
}


@await Html.PartialAsync("Question/YourEmployees", Model.SubSections.Single(w => w.Id =="YourEmployees"))
@await Html.PartialAsync("Question/YourApprentices", Model.SubSections.Single(w => w.Id =="YourApprentices"))
@if (Model.SubSections.SingleOrDefault(w => w.Id == "FullTimeEquivalent") != null)
{
    @await Html.PartialAsync("Question/FullTimeEquivalent", Model.SubSections.Single(w => w.Id =="FullTimeEquivalent"))
}
